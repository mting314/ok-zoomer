{
  "name": "Ok, Zoomer",
  "version": "0.0.0.1",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "declarativeContent", "storage", "unlimitedStorage", "alarms", "notifications", "tabs"],
  "options_page": "options/index.html",
  "background": {
    "page": "background/background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://ucla.zoom.us/*"],
      "js": ["utils.js", "jquery-3.4.1.min.js", "injectZoom.js"],
      "css": ["zoom.css"]
    },
    {
      "matches": ["https://be.my.ucla.edu/ClassPlanner/ClassPlan.aspx"],
      "js": ["utils.js", "jquery-3.4.1.min.js", "injectplanner/injectPlanner.js", "injectplanner/highlightCurrent.js", "injectplanner/injectAdding.js"],
      "css" : ["injectplanner/planner.css"]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "web_accessible_resources": ["fonts/*"],
  "manifest_version": 2
}
